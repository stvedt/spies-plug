{
  "manifest_version": 2,

  "name": "plug.dj Backgrounds",
  "description": "This extension adds custom backgrounds to plug.dj",
  "version": "0.0.1",
  "default_locale": "en",
  "icons": { "128": "img/icon-128.png" },

  "content_scripts": [
    {
      "matches": ["http://plug.dj/80f368f6/"],
      "css": ["style.css"]
    }
  ],
  "permissions": [
    "http://plug.dj/"
  ]

}
