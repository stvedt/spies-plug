{
  "manifest_version": 2,

  "name": "Spies Plug",
  "description": "This extension adds the Spies Background to the Spies Plug.dj room",
  "version": "0.0.",
  "icons": { "128": "img/icon-128.png" },

  "content_scripts": [
    {
      "matches": ["http://plug.dj/80f368f6/"],
      "css": ["style.css"],
      "js": ["inject.js"]
    }
  ],
  "permissions": [
    "http://plug.dj/",
    "notifications"
  ],
  "web_accessible_resources": ["main.js"]

}
