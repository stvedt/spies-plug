{
  "manifest_version": 2,

  "name": "Spies Plug",
  "description": "This extension adds the Spies Background to the Spies Plug.dj room",
  "version": "0.1.0",
  "icons": { "128": "img/icon-128.png" },

  "content_scripts": [
    {
      "matches": ["https://plug.dj/80f368f6/"],
      "css": ["style.css"]
    }
  ],
  "permissions": [
    "https://plug.dj/",
    "notifications"
  ]

}
